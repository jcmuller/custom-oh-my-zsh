alias pg_base_10='bundle exec docker run -it --network=host --rm -e PGHOST -e PGUSER -e PGPASSWORD -v /tmp:/tmp:ro postgres:10-alpine'
alias createdb='pg_base_10 createdb'
alias pg_restore='pg_base_10 pg_restore'
alias dropdb='pg_base_10 dropdb'
alias psql='pg_base_10 psql'
alias pg_dump='pg_base_10 pg_dump'
alias pgsh='pg_base_10 sh'
